#wrap
  margin:0 auto
  width:290px
  //height:200px
  text-align:center
  text-shadow: 2px 2px 2px #ccc
  font-size: 17px
  font-weight: bold
  border:1px solid #cccccc
  -moz-border-radius: 7px
